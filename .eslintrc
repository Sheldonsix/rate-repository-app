{
    "plugins": [
        "react",
        "react-native"
    ],
    "settings": {
        "react": {
            "version": "detect"
        },
        "import/resolver": {
            "node": {
                "extensions": [
                    ".ts",
                    ".tsx",
                    ".native.js"
                ]
            }
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jest/recommended"
    ],
    "parser": "@babel/eslint-parser",
    // "parser": "babel-eslint",
    "env": {
        "react-native/react-native": true
    },
    "rules": {
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off"
    }
}